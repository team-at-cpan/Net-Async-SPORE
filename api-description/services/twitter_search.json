{
    "name" : "twitter search",
    "base_url" : "http://search.twitter.com/",
    "methods" : {
        "search" : {
            "path" : "/search.:format",
            "method" : "GET",
            "required_params" : [
                "format",
                "q"
            ],
            "optional_params" : [
                "geocode",
                "lang",
                "locale",
                "page",
                "result_type"
             ]
        }
    }
}
